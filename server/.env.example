# Serverio konfigūracija
PORT=5000
NODE_ENV=development

# MongoDB prisijungimo duomenys
MONGODB_URI=mongodb://localhost:27017/nt-analize

# Scraper konfigūracija
RUN_SCRAPER_ON_START=false
SCRAPER_INTERVAL_HOURS=24

# Geokodavimo servisas
GEOCODER_PROVIDER=openstreetmap
GEOCODER_API_KEY=

# Aruodas scraper konfigūracija
ARUODAS_SCRAPER_PATH=../scripts/scraper
ARUODAS_CITIES=Vilnius,Kaunas,Klaipėda,Šiauliai,Panevėžys
ARUODAS_PROPERTY_TYPES=flat,house,land

# Laiko juosta
TZ=Europe/Vilnius